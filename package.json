{
  "name": "mqtt-ws-proxy",
  "version": "v1.2.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register 'test/unit/**/*.spec.js'",
    "test:e2e": "mocha --compilers js:babel-register 'test/e2e/**/*.spec.js'",
    "build": "babel src/ --out-dir dist --source-maps",
    "clean": "find ./dist -name *.js -type f -delete",
    "start": "node dist/index.js ",
    "dev": "nodemon --exec babel-node src/index.js",
    "lint": "eslint ./ --cache ",
    "postinstall": "./scripts/postinstall.sh",
    "precommit": "npm run lint && npm run test",
    "validate-commit": "validate-commit-msg",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert",
        "release"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "autoFix": false,
      "scope": {
        "required": false
      }
    }
  },
  "precommit": [
    "lint",
    "test"
  ],
  "keywords": [],
  "author": "Fabrizio Guglielmino <guglielmino@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-node6": "^0.3.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "commitizen": "^2.9.6",
    "eslint": "^3.17.1",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-plugin-import": "^2.2.0",
    "expect": "^1.20.2",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.2.2",
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.6",
    "validate-commit-msg": "^2.14.0"
  },
  "dependencies": {
    "daemon": "^1.1.0",
    "dotenv": "^4.0.0",
    "lodash": "^4.17.2",
    "mqtt": "^2.0.1",
    "pubnub": "^4.2.5",
    "request": "^2.81.0",
    "request-promise": "^4.1.1",
    "winston": "^2.3.0"
  }
}
